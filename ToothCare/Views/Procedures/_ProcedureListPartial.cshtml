@model ToothCare.Models.ViewModel<ProceduresDTO>

<div class="row" id="procedureContainer">
    @foreach (var item in Model.lists)
    {
        <div class="col-md-4 mb-4">
            <div class="square bg-dark p-4 text-bg-danger">
                <h3 class="text-light">@item.Name</h3>
                <p class="text-light">Cost: @item.Price</p>
            </div>
        </div>
    }
</div>
